{
  "name": "styles-highlighter",
  "main": "./lib/styles-highlighter",
  "version": "2.3.0",
  "description": "A styles highlighter plugin for the Atom text editor. It highlights with a specific color all occurrences of the selected string in the current document.",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "styles-highlighter:markFirstStyle",
      "styles-highlighter:markSecondStyle",
      "styles-highlighter:markThirdStyle",
      "styles-highlighter:markFourthStyle",
      "styles-highlighter:markFifthStyle",
      "styles-highlighter:markSixthStyle",
      "styles-highlighter:markSeventhStyle",
      "styles-highlighter:markEighthStyle",
      "styles-highlighter:markNinethStyle",
      "styles-highlighter:markTenthStyle",
      "styles-highlighter:clearFirstStyle",
      "styles-highlighter:clearSecondStyle",
      "styles-highlighter:clearThirdStyle",
      "styles-highlighter:clearFourthStyle",
      "styles-highlighter:clearFifthStyle",
      "styles-highlighter:clearSixthStyle",
      "styles-highlighter:clearSeventhStyle",
      "styles-highlighter:clearEighthStyle",
      "styles-highlighter:clearNinethStyle",
      "styles-highlighter:clearTenthStyle",
      "styles-highlighter:clearAllStyles",
      "styles-highlighter:toggleOccurenceHighlighter"
    ]
  },
  "repository": "https://github.com/Tuchkata/styles-highlighter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {}
}
